{"ast":null,"code":"import { getItineraryById } from '../services/itinerary.service';\nexport default {\n  name: 'Itinerary-chart',\n  data() {\n    return {\n      currentId: 1,\n      itineraryData: null\n    };\n  },\n  methods: {\n    getItineraryById(id) {\n      if (id) this.currentId = id;\n      getItineraryById(this.currentId).then(data => this.itineraryData = data);\n    }\n  },\n  beforeMount() {\n    this.getItineraryById();\n  },\n  computed: {\n    myStyles() {\n      return {\n        height: `${50}px`\n      };\n    }\n  }\n};","map":{"version":3,"mappings":"AA8CA,SAASA,gBAAe,QAAS,+BAA+B;AAEhE,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,IAAI,GAAG;IACL,OAAO;MACLC,SAAS,EAAE,CAAC;MACZC,aAAa,EAAE;IACjB;EACF,CAAC;EACDC,OAAO,EAAE;IACPL,gBAAgB,CAACM,EAAE,EAAE;MACnB,IAAIA,EAAE,EAAE,IAAI,CAACH,SAAQ,GAAIG,EAAE;MAC3BN,gBAAgB,CAAC,IAAI,CAACG,SAAS,CAAC,CAACI,IAAI,CAACL,IAAG,IAAK,IAAI,CAACE,aAAY,GAAIF,IAAI,CAAC;IAC1E;EACF,CAAC;EACDM,WAAW,GAAE;IACX,IAAI,CAACR,gBAAgB,EAAC;EACxB,CAAC;EACDS,QAAQ,EAAE;IACRC,QAAO,GAAK;MACV,OAAO;QACLC,MAAM,EAAG,GAAE,EAAG;MAChB;IACF;EACF;AACF","names":["getItineraryById","name","data","currentId","itineraryData","methods","id","then","beforeMount","computed","myStyles","height"],"sourceRoot":"","sources":["/Users/Josh/Trabajo/aerialoop/aerialoop-frontend/src/components/ItineraryCharts.vue"],"sourcesContent":["<template>\n  <img alt=\"logo\" src=\"../assets/logo.png\">\n  <div>\n    <h3>Flight Itineraries</h3>\n    <p>\n      This app allows you to check essential information on different itineraries.\n    </p>\n    <h3>Available itineraries</h3>\n    <ul>\n      <li><button :class=\"[currentId == 1 ? 'selected' : '']\" @click=\"getItineraryById(1)\">Itinerary #1</button></li>\n      <li><button :class=\"[currentId == 2 ? 'selected' : '']\" @click=\"getItineraryById(2)\">Itinerary #2</button></li>\n      <li><button :class=\"[currentId == 3 ? 'selected' : '']\" @click=\"getItineraryById(3)\">Itinerary #3</button></li>\n      <li><button :class=\"[currentId == 4 ? 'selected' : '']\" @click=\"getItineraryById(4)\">Itinerary #4</button></li>\n      <li><button :class=\"[currentId == 5 ? 'selected' : '']\" @click=\"getItineraryById(5)\">Itinerary #5</button></li>\n    </ul>\n    <p>\n      <strong>From:</strong> {{ itineraryData.from }} <strong>- To:</strong> {{ itineraryData.to }}\n    </p>\n    <p>\n      <strong>Day starting time in milliseconds since midnight:</strong> {{ itineraryData.startDayTime }}\n    </p>\n    <h3>Flight Data</h3>\n    <table>\n      <tr>\n        <th>TIME_StartTime</th>\n        <th>BATT_VFilt</th>\n        <th>BATT_CFilt</th>\n        <th>BATT_Discharged</th>\n        <th>BATT_Remaining</th>\n        <th>BATT_RemainingV</th>\n        <th>BATT_Warning</th>\n      </tr>\n      <tr v-for=\"flight in itinerary.flightData\">\n        <td>{{ flight.TIME_StartTime }}</td>\n        <td>{{ flight.BATT_VFilt }}</td>\n        <td>{{ flight.BATT_CFilt }}</td>\n        <td>{{ flight.BATT_Discharged }}</td>\n        <td>{{ flight.BATT_Remaining }}</td>\n        <td>{{ flight.BATT_RemainingV }}</td>\n        <td>{{ flight.BATT_Warning }}</td>\n      </tr>\n    </table>\n  </div>\n</template>\n\n<script>\nimport { getItineraryById } from '../services/itinerary.service';\n\nexport default {\n  name: 'Itinerary-chart',\n  data() {\n    return {\n      currentId: 1,\n      itineraryData: null,\n    }\n  },\n  methods: {\n    getItineraryById(id) {\n      if (id) this.currentId = id;\n      getItineraryById(this.currentId).then(data => this.itineraryData = data);\n    }\n  },\n  beforeMount(){\n    this.getItineraryById()\n  },\n  computed: {\n    myStyles () {\n      return {\n        height: `${50}px`,\n      }\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nimg {\n  width: 15%;\n}\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\nbutton {\n  margin-bottom: 25px;\n  border-radius: 12px;\n  border: 2px solid #0A2036;\n  padding: 14px 40px;\n  background-color: white;\n  color: #0A2036;\n  transition-duration: 0.4s;\n}\nbutton:hover, .selected{\n  background-color: #0A2036;\n  color: white;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}